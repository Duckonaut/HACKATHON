TAB DC 10*INTEGER
SIZE DC INTEGER(10)
STEP DC INTEGER(1)
MEM_STEP DC INTEGER(4)
ZERO DC INTEGER(0)
ONE DC INTEGER(1)
 L 1, SIZE
 S 1, ONE
OUT L 2, ZERO
IN LR 10, 2
 M 10, MEM_STEP
 L 3, 0(10)
 C 3, 4(10)
 JP SWAP
 J NEXT_STEP_IN
SWAP L 4, 4(10)
 ST 4, 0(10)
 ST 3, 4(10)
NEXT_STEP_IN A 2, STEP
 CR 2, 1
 JN IN
NEXT_STEP_OUT S 1, STEP
 C 1, ZERO
 JP OUT
END CR 1,1
